<?xml version="1.0" encoding="UTF-8"?>
<Syntax xmlns="urn:fdc:difi.no:2017:vefa:structure:Syntax-1">
    <Term>order-cancellation</Term>
    <Name>Order Cancellation</Name>

    <Property key="sch:prefix">EHF-ORDERCANCELLATION</Property>
    <Property key="sch:identifier">basic-order-cancellation</Property>

    <Namespace prefix="cac">urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2</Namespace>
    <Namespace prefix="cbc">urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2</Namespace>
    <Namespace prefix="ubl">urn:oasis:names:specification:ubl:schema:xsd:OrderCancellation-2</Namespace>

    <Document>
        <Term>ubl:OrderCancellation</Term>
        <Name>Order Cancellation</Name>
        <Description>Order cancellation format for the Norwegian market based on CEN BII</Description>

        <Element>
            <Term>cbc:UBLVersionID</Term>
            <Name>UBL Version</Name>
            <Description>UBL version used in the message</Description>
            <Value type="FIXED">2.2</Value>
        </Element>

        <Element>
            <Term>cbc:CustomizationID</Term>
            <Name>Customization identifier</Name>
            <Description>Identifies the specification of content and rules that apply to the transaction.</Description>
            <Value type="FIXED">urn:fdc:difi.no:2017:ehf:order-cancellation:3.0-pilot</Value>
        </Element>

        <Include>part/document/profile.xml</Include>

        <Element>
            <Term>cbc:ID</Term>
            <Name>Order identifier</Name>
            <Description>A transaction instance must have an identifier. The identifier enables referencing the transaction  for various purposes such as from other transactions that are part of the same process.</Description>
            <Value type="EXAMPLE">01</Value>
        </Element>
        
        <Element>
            <Term>cbc:IssueDate</Term>
            <Name>Order issue date</Name>
            <Description>The date on which the transaction instance was issued. Format YYYY-MM-DD.</Description>
            <Value type="EXAMPLE">2012-11-25</Value>
        </Element>
        
        <Element cardinality="0..1">
            <Term>cbc:IssueTime</Term>
            <Name>Order issue time</Name>
            <Description>The time assigned by the buyer on which the order was issued.</Description>
            <Value type="EXAMPLE">12:00</Value>
        </Element>

        <Element cardinality="0..1">
            <Term>cbc:Note</Term>
            <Name>Document level textual note</Name>
            <Description>Free form text applying to the Order. This element may contain notes or any other similar information that is not contained explicitly in another structure.</Description>
        </Element>
        
        <Element>
            <Term>cac:OrderReference</Term>
            <Name>Order reference</Name>

            <Element>
                <Term>cbc:ID</Term>
                <Name>Order reference identifier</Name>
                <Description>Reference to original order.</Description>
                <Value type="EXAMPLE">5678</Value>
            </Element>
        </Element>
        
        <Element cardinality="0..1">
            <Term>cac:OriginatorDocumentReference</Term>
            <Description>Information about Originator Document reference</Description>
            
            <Element>
                <Term>cbc:ID</Term>
                <Name>Originator document reference</Name>
                <Description>A reference to Originator Document. To be able to give a reference to the internal requesition on the buyer site on which the order is based.</Description>
                <Value type="EXAMPLE">Dok_789</Value>
            </Element>
            
            <Element cardinality="0..1">
                <Term>cbc:DocumentType</Term>
                <Name>Originator document description</Name>
                <Description>Originator document description.</Description>
                <Value type="EXAMPLE">Foresp√∏rsel fra mai 2012</Value>
            </Element>
        </Element>
        
        <Element cardinality="0..n">
            <Term>cac:AdditionalDocumentReference</Term>
            <Description>Information about Additional Document reference</Description>
            
            <Element>
                <Term>cbc:ID</Term>
                <Name>Document identifier</Name>
                <Description>An identifier for the referenced document.</Description>
                <Value type="EXAMPLE">Vedlegg_1</Value>
            </Element>

            <Element cardinality="0..1">
                <Term>cbc:DocumentType</Term>
                <Name>Document description</Name>
                <Description>A short description of the document type.</Description>
                <Value type="EXAMPLE">Tegning</Value>
            </Element>

            <Element cardinality="0..1">
                <Term>cac:Attachment</Term>
                <Description>Information about attachment</Description>
                
                <Element cardinality="0..1">
                    <Term>cbc:EmbeddedDocumentBinaryObject</Term>
                    <Name>Attached binary object</Name>
                    <Description>The attached document embeded as binary object.</Description>

                    <Attribute>
                        <Term>mimeCode</Term>
                        <Value type="EXAMPLE">application/pdf</Value>
                    </Attribute>
                </Element>
                
                <Element cardinality="0..1">
                    <Term>cac:ExternalReference</Term>
                    <Name>Information about external reference/URI</Name>
                    
                    <Element>
                        <Term>cbc:URI</Term>
                        <Name>External document URI</Name>
                        <Description>The Uniform Resource Identifier (URI) that identifies where the external document is located.</Description>
                        <Value type="EXAMPLE">https://www.example.com/document.pdf</Value>
                    </Element>
                </Element>
            </Element>
        </Element>
        
        <Element cardinality="0..1">
            <Term>cac:Contract</Term>
            <Description>Information about Contract</Description>
            
            <Element>
                <Term>cbc:ID</Term>
                <Name>Reference identifier</Name>
                <Description>Positive identification of the reference such as a unique identifier.</Description>
                <Value type="EXAMPLE">2012_10</Value>
            </Element>
            
            <Element cardinality="0..1">
                <Term>cbc:ContractType</Term>
                <Name>Reference type</Name>
                <Description>The short description of what is reference such as contract type, document type , meter etc.
                    The type of Contract, expressed as text.</Description>
                <Value type="EXAMPLE">Rammeavtale</Value>
            </Element>
        </Element>
        
        <Element>
            <Term>cac:BuyerCustomerParty</Term>
            <Description>Information about Buyer</Description>

            <Element>
                <Term>cac:Party</Term>
                <Description>Partyinformation</Description>
                
                <Element cardinality="0..1">
                    <Term>cbc:EndpointID</Term>
                    <Name>Buyer party endpoint identifier</Name>
                    <Description>Identifies the end point of the routing service.</Description>

                    <Attribute>
                        <Term>schemeID</Term>
                        <Description>Must be according to "PEPPOL policy for identifiers".</Description>
                        <Reference type="CODE_LIST">ICD</Reference>
                        <Value type="EXAMPLE">NO:ORGNR</Value>
                    </Attribute>

                    <Value type="EXAMPLE">931186755</Value>
                </Element>
                
                <Element cardinality="0..1">
                    <Term>cac:PartyIdentification</Term>
                    <Description>Information about PartyID</Description>

                    <Element>
                        <Term>cbc:ID</Term>
                        <Name>Buyer party identification</Name>
                        <Description>An identification for the buyer party.</Description>

                        <Attribute>
                            <Term>schemeID</Term>
                            <Description>Must be according to "PEPPOL policy for identifiers".</Description>
                            <Reference type="CODE_LIST">ICD</Reference>
                            <Reference type="CODE_LIST">ICD_UNKNOWN</Reference>
                            <Value type="EXAMPLE">GLN</Value>
                        </Attribute>

                        <Value type="EXAMPLE">7080000985134</Value>
                    </Element>
                </Element>

                <Element cardinality="0..1">
                    <Term>cac:PartyName</Term>
                    <Description>Information about Party name</Description>
                    
                    <Element>
                        <Term>cbc:Name</Term>
                        <Name>Buyer party name</Name>
                        <Description>The name of the party who orders the listed items. </Description>
                        <Value type="EXAMPLE">Helseforetak</Value>
                    </Element>
                </Element>

                <Include>part/party-tax-scheme.xml</Include>

                <Include>part/postal-address.xml</Include>

                <Element cardinality="0..1">
                    <Term>cac:PartyTaxScheme</Term>
                    <Description>Information about VAT</Description>

                    <Element>
                        <Term>cbc:CompanyID</Term>
                        <Name>Buyer VAT identifier</Name>
                        <Description>The buyers registered  Value Added Tax identifier. To be stated in case reverse charge is to apply to the purchase.</Description>

                        <Attribute>
                            <Term>schemeID</Term>
                            <Description>Must be according to "PEPPOL policy for identifiers".</Description>
                            <Reference type="CODE_LIST">ICD</Reference>
                            <Value type="EXAMPLE">NO:VAT</Value>
                        </Attribute>

                        <Value type="EXAMPLE">931186755MVA</Value>
                    </Element>

                    <Element>
                        <Term>cac:TaxScheme</Term>
                        <Description>Information about Tax scheme</Description>

                        <Element>
                            <Term>cbc:ID</Term>
                            <Value type="FIXED">VAT</Value>
                        </Element>
                    </Element>
                </Element>

                <Element cardinality="0..1">
                    <Term>cac:PartyLegalEntity</Term>
                    <Description>Information about Legal entity</Description>

                    <Element cardinality="0..1">
                        <Term>cbc:RegistrationName</Term>
                        <Name>Buyers legal registration name</Name>
                        <Description>The official name of the party as registered with the relevant fiscal authority.</Description>
                        <Value type="EXAMPLE">Helseforetak AS</Value>
                    </Element>

                    <Element cardinality="0..1">
                        <Term>cbc:CompanyID</Term>
                        <Name>Buyers legal registration identifier</Name>
                        <Description>Identifies a company as registered with the company registration scheme.</Description>

                        <Attribute>
                            <Term>schemeID</Term>
                            <Description>Must be according to "PEPPOL policy for identifiers".</Description>
                            <Reference type="CODE_LIST">ICD</Reference>
                            <Value type="EXAMPLE">NO:ORGNR</Value>
                        </Attribute>

                        <Value type="EXAMPLE">931186755</Value>
                    </Element>

                    <Element cardinality="0..1">
                        <Term>cac:RegistrationAddress</Term>
                        <Description>Information about Registration address</Description>

                        <Element cardinality="0..1">
                            <Term>cbc:CityName</Term>
                            <Name>Buyers legal registration address city name</Name>
                            <Description>Associates with the registered address of the party within a Corporate Registration Scheme. The name of a city, town, or village.</Description>
                            <Value type="EXAMPLE">Oslo</Value>
                        </Element>

                        <Element cardinality="0..1">
                            <Term>cac:Country</Term>
                            <Description>Information about Country</Description>

                            <Element>
                                <Term>cbc:IdentificationCode</Term>
                                <Name>Buyers legal registration address country code</Name>
                                <Description>Buyers legal registration address country ID</Description>
                                <Reference type="CODE_LIST">ISO3166</Reference>

                                <Attribute>
                                    <Term>listID</Term>
                                    <Value type="FIXED">ISO3166-1:Alpha2</Value>
                                </Attribute>

                                <Value type="EXAMPLE">NO</Value>
                            </Element>
                        </Element>
                    </Element>
                </Element>

                <Include>part/contact.xml</Include>

            </Element>
        </Element>
        
        <Element>
            <Term>cac:SellerSupplierParty</Term>
            <Description>Information about Seller</Description>
            
            <Element>
                <Term>cac:Party</Term>
                <Description>Partyinformation</Description>
                
                <Element cardinality="0..1">
                    <Term>cbc:EndpointID</Term>
                    <Name>Seller party endpoint identifier</Name>
                    <Description>Identifies the end point of the routing service.</Description>

                    <Attribute>
                        <Term>schemeID</Term>
                        <Description>Must be according to "PEPPOL policy for identifiers".</Description>
                        <Reference type="CODE_LIST">ICD</Reference>
                        <Value type="EXAMPLE">NO:ORGNR</Value>
                    </Attribute>
                    
                    <Value type="EXAMPLE">938752655</Value>
                </Element>
                
                <Element cardinality="0..1">
                    <Term>cac:PartyIdentification</Term>
                    <Description>Information about PartyID</Description>
                    
                    <Element>
                        <Term>cbc:ID</Term>
                        <Name>Seller party identification</Name>
                        <Description>Identifies a party.</Description>

                        <Attribute>
                            <Term>schemeID</Term>
                            <Description>Must be according to "PEPPOL policy for identifiers".</Description>
                            <Reference type="CODE_LIST">ICD</Reference>
                            <Reference type="CODE_LIST">ICD_UNKNOWN</Reference>
                            <Value type="EXAMPLE">GLN</Value>
                        </Attribute>

                        <Value type="EXAMPLE">7080000374200</Value>
                    </Element>
                </Element>
                
                <Element cardinality="0..1">
                    <Term>cac:PartyName</Term>
                    <Description>Information about Party name</Description>
                    
                    <Element>
                        <Term>cbc:Name</Term>
                        <Name>Seller party name</Name>
                        <Description>The name of the party.</Description>
                        <Value type="EXAMPLE">Medical</Value>
                    </Element>
                </Element>

                <Include>part/postal-address.xml</Include>

                <Include>part/contact.xml</Include>
            </Element>
        </Element>
        
        <Element cardinality="0..1">
            <Term>cac:OriginatorCustomerParty</Term>
            <Description>Information about Originator</Description>
            
            <Element>
                <Term>cac:Party</Term>
                <Description>Party information</Description>

                <Element cardinality="0..1">
                    <Term>cac:PartyIdentification</Term>
                    <Description>Information about PartyID</Description>

                    <Element>
                        <Term>cbc:ID</Term>
                        <Name>Originator identifier</Name>
                        <Description>Identifies a party.</Description>
                        <Reference type="CODE_LIST">ICD</Reference>
                        <Reference type="CODE_LIST">ICD_UNKNOWN</Reference>

                        <Attribute>
                            <Term>schemeID</Term>
                            <Description>Must be according to "PEPPOL policy for identifiers".</Description>
                            <Value type="EXAMPLE">GLN</Value>
                        </Attribute>

                        <Value type="EXAMPLE">12345</Value>
                    </Element>
                </Element>

                <Element cardinality="0..1">
                    <Term>cac:PartyName</Term>
                    <Description>Information about Party name</Description>

                    <Element>
                        <Term>cbc:Name</Term>
                        <Name>Originator name</Name>
                        <Description>The name of the party.</Description>
                        <Value type="EXAMPLE">Helseavdeling</Value>
                    </Element>
                </Element>

                <Include>part/contact.xml</Include>
            </Element>
        </Element>
        
    </Document>
</Syntax>