<?xml version="1.0" encoding="UTF-8"?>
<Structure xmlns="urn:fdc:difi.no:2017:vefa:structure-1">
    <Term>order-response-simple</Term>
    <Name>Order Response (Simple)</Name>

    <Property key="sch:prefix">EHF-ORDERRESPONSESIMPLE</Property>
    <Property key="sch:identifier">basic-order-response-simple</Property>

    <Namespace prefix="cac">urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2</Namespace>
    <Namespace prefix="cbc">urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2</Namespace>
    <Namespace prefix="ubl">urn:oasis:names:specification:ubl:schema:xsd:OrderResponseSimple-2</Namespace>

    <Document>
        <Term>ubl:OrderResponseSimple</Term>
        <Name>Order Response (Simple)</Name>
        <Description>Norwegian implementation of the CEN BII Order respons</Description>

        <Element>
            <Term>cbc:UBLVersionID</Term>
            <Name>UBL Version</Name>
            <Description>UBL version used in the message</Description>
            <Value type="FIXED">2.1</Value>
        </Element>

        <Element>
            <Term>cbc:CustomizationID</Term>
            <Name>Customization identifier</Name>
            <Description>Identifies the specification of content and rules that apply to the transaction.</Description>
            <Value type="FIXED">urn:www.cenbii.eu:transaction:biitrns076:ver2.0: extended:urn:www.peppol.eu:bis:peppol28a:ver1.0: extended:urn:www.difi.no:ehf:ordrebekreftelse:ver1.0</Value>
        </Element>
        
        <Element>
            <Term>cbc:ProfileID</Term>
            <Name>Profile identifier</Name>
            <Description>Identifies the BII profile or business process context in which the transaction appears.</Description>
            <Value type="FIXED">urn:www.cenbii.eu:profile:bii28:ver2.0</Value>
        </Element>

        <Element>
            <Term>cbc:ID</Term>
            <Name>Order identifier</Name>
            <Description>A transaction instance must have an identifier. The identifier enables referencing the transaction  for various purposes such as from other transactions that are part of the same process.</Description>
            <Value type="EXAMPLE">12</Value>
        </Element>
        
        <Element>
            <Term>cbc:IssueDate</Term>
            <Name>Order response issue date</Name>
            <Description>The date on which the transaction instance was issued. Format YYYY-MM-DD.</Description>
            <Value type="EXAMPLE">2012-11-25</Value>
        </Element>
        
        <Element cardinality="0..1">
            <Term>cbc:IssueTime</Term>
            <Name>Order response issue time</Name>
            <Description>The time assigned by the buyer on which the order was issued.</Description>
            <Value type="EXAMPLE">12:00</Value>
        </Element>

        <Element cardinality="0..1">
            <Term>cbc:Note</Term>
            <Name>Response clarification</Name>
            <Description>Clarifacation of the suppliers decision.</Description>
            <Value type="EXAMPLE">Endring p√• 2 linjer</Value>
        </Element>

        <Element>
            <Term>cbc:AcceptedIndicator</Term>
            <Name>Accepted indicator</Name>
            <Description>Indication of acceptance.</Description>
        </Element>

        <Element cardinality="0..1">
            <Term>cbc:RejectionNote</Term>
            <Name>Note of rejection</Name>
        </Element>

        <Element>
            <Term>cac:OrderReference</Term>
            <Name>OrderReference</Name>
            <Description>Order reference information</Description>

            <Element>
                <Term>cbc:ID</Term>
                <Name>Order document reference</Name>
                <Description>Used to reference the order that  is being responded to.</Description>
                <Value type="EXAMPLE">24</Value>
            </Element>
        </Element>

        <Element>
            <Term>cac:SellerSupplierParty</Term>
            <Description>Information about Seller</Description>

            <Element>
                <Term>cac:Party</Term>
                <Description>Party information</Description>

                <Element cardinality="0..1">
                    <Term>cbc:EndpointID</Term>
                    <Name>Seller party endpoint identifier</Name>
                    <Description>Identifies the end point of the routing service, e.g., EAN Location Number, GLN.</Description>

                    <Attribute>
                        <Term>schemeID</Term>
                        <Description>Must be according to "PEPPOL policy for identifiers".</Description>
                        <Reference type="CODE_LIST">ICD</Reference>
                        <Value type="EXAMPLE">NO:ORGNR</Value>
                    </Attribute>

                    <Value type="EXAMPLE">938752655</Value>
                </Element>

                <Element cardinality="0..n">
                    <Term>cac:PartyIdentification</Term>
                    <Description>Information about PartyID</Description>

                    <Element>
                        <Term>cbc:ID</Term>
                        <Name>Seller party identification</Name>
                        <Description>Identifies a party.</Description>

                        <Attribute>
                            <Term>schemeID</Term>
                            <Description>Must be according to "PEPPOL policy for identifiers".</Description>
                            <Reference type="CODE_LIST">ICD</Reference>
                            <Reference type="CODE_LIST">ICD_UNKNOWN</Reference>
                            <Value type="EXAMPLE">GLN</Value>
                        </Attribute>

                        <Value type="EXAMPLE">7080000374200</Value>
                    </Element>
                </Element>

                <Element cardinality="0..1">
                    <Term>cac:PartyName</Term>
                    <Description>Information about Party name</Description>

                    <Element>
                        <Term>cbc:Name</Term>
                        <Name>Seller party name</Name>
                        <Description>The name of the party.</Description>
                        <Value type="EXAMPLE">Medical</Value>
                    </Element>
                </Element>

                <Include>part/contact.xml</Include>
            </Element>
        </Element>

        <Element>
            <Term>cac:BuyerCustomerParty</Term>
            <Description>Information about Buyer</Description>

            <Element>
                <Term>cac:Party</Term>
                <Description>Partyinformation</Description>
                
                <Element cardinality="0..1">
                    <Term>cbc:EndpointID</Term>
                    <Name>Buyer party endpoint identifier</Name>
                    <Description>Identifies the end point of the routing service.</Description>

                    <Attribute>
                        <Term>schemeID</Term>
                        <Description>Must be according to "PEPPOL policy for identifiers".</Description>
                        <Reference type="CODE_LIST">ICD</Reference>
                        <Value type="EXAMPLE">NO:ORGNR</Value>
                    </Attribute>

                    <Value type="EXAMPLE">931186755</Value>
                </Element>
                
                <Element cardinality="0..n">
                    <Term>cac:PartyIdentification</Term>
                    <Description>Information about PartyID</Description>

                    <Element>
                        <Term>cbc:ID</Term>
                        <Name>Buyer party identification</Name>
                        <Description>An identification for the buyer party.</Description>

                        <Attribute>
                            <Term>schemeID</Term>
                            <Description>Must be according to "PEPPOL policy for identifiers".</Description>
                            <Reference type="CODE_LIST">ICD</Reference>
                            <Reference type="CODE_LIST">ICD_UNKNOWN</Reference>
                            <Value type="EXAMPLE">GLN</Value>
                        </Attribute>

                        <Value type="EXAMPLE">7080000985134</Value>
                    </Element>
                </Element>

                <Element cardinality="0..1">
                    <Term>cac:PartyName</Term>
                    <Description>Information about Party name</Description>
                    
                    <Element>
                        <Term>cbc:Name</Term>
                        <Name>Buyer party name</Name>
                        <Description>The name of the party who orders the listed items. </Description>
                        <Value type="EXAMPLE">Helseforetak</Value>
                    </Element>
                </Element>

                <Element cardinality="0..1">
                    <Term>cac:Contact</Term>
                    <Name>Contact</Name>
                    <Description>Contact information</Description>

                    <Element>
                        <Term>cbc:ID</Term>
                        <Name>Contact ID</Name>
                        <Description>Contact identifier for buyer contact person</Description>
                    </Element>
                </Element>

            </Element>
        </Element>
    </Document>
</Structure>